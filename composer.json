{
  "name": "phalcon-kit/app",
  "version": "1.0.0",
  "type": "project",
  "description": "Start a new Phalcon Kit Project",
  "keywords": [
    "php",
    "framework",
    "php-framework",
    "phalcon",
    "phalcon-app",
    "phalcon-php",
    "phalcon-kit",
    "phalcon-kit-core",
    "phalcon-kit-app",
    "phalcon-toolkit"
  ],
  "homepage": "https://github.com/phalcon-kit",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Julien Turbide",
      "email": "contact@jturbide.com",
      "homepage": "https://www.jturbide.com/"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/phalcon-kit/core/graphs/contributors"
    }
  ],
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/jturbide"
    }
  ],
  "support": {
    "issues": "https://github.com/phalcon-kit/core/issues",
    "source": "https://github.com/phalcon-kit/core"
  },
  "config": {
    "optimize-autoloader": true,
    "apcu-autoloader": true,
    "apcu-autoloader-prefix": "phalcon-kit-",
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "cweagans/composer-patches": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.4",
    "ext-phalcon": ">=5.9.3",
    "phalcon-kit/core": "^1.0 || dev-master"
  },
  "require-dev": {
    "phalcon/ide-stubs": "^5.9.3"
  },
  "extra": {
    "installer-paths": {
      "public/vendor/{$name}": [
        "phalcon-kit/workspace"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Unit\\": "tests/Unit",
      "PhalconKit\\Tests\\Unit\\": "vendor/phalcon-kit/core/tests"
    }
  },
  "bin":[]
}
